   "source": [
    "import cv2\n",
    "cap = cv2.VideoCapture(0)\n",
    "while True: \n",
    "    status , photo = cap.read ()\n",
    "    newphoto = photo[100:500, 100:500]\n",
    "    cv2.imshow('hi2',newphoto)\n",
    "    if cv2.waitKey(1) == 27:\n",
    "        break\n",
    "cv2.destroyAllWindows()\n",
    "cap.release()"
   ]
  }
